{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "NODE_ENV",
    "DATABASE_URL",
    "DATABASE_DIRECT_URL",
    "DIRECT_URL",
    "SUPABASE_URL",
    "SUPABASE_ANON_KEY",
    "SUPABASE_SERVICE_KEY",
    "SUPABASE_SERVICE_ROLE_KEY",
    "ANTHROPIC_API_KEY",
    "CLERK_SECRET_KEY",
    "CLERK_WEBHOOK_SECRET",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL",
    "UPSTASH_REDIS_REST_URL",
    "UPSTASH_REDIS_REST_TOKEN",
    "BROWSERLESS_API_KEY",
    "BROWSERLESS_URL",
    "CRON_SECRET",
    "AWE_SUPABASE_URL",
    "AWE_SUPABASE_ANON_KEY",
    "AWE_SUPABASE_SERVICE_KEY",
    "AWE_CACHE_SIZE",
    "AWE_MAX_CONCURRENCY",
    "AWE_API_TIMEOUT",
    "AWE_API_RETRIES",
    "AWE_FEATURES",
    "AWE_OFFLINE_MODE",
    "AWE_TELEMETRY_ENABLED",
    "AWE_CRASH_REPORTING",
    "AWE_DEBUG",
    "AWE_LOG_LEVEL",
    "FIRECRAWL_API_KEY",
    "NOTIFICATION_WEBHOOK_URL",
    "CUSTOM_KEY",
    "SKIP_ENV_VALIDATION",
    "DEBUG"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": [
        "NODE_ENV",
        "DATABASE_URL",
        "DIRECT_URL"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "type-check": {
      "dependsOn": ["^build"]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}