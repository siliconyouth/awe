{
  "name": "@awe/cli",
  "version": "1.0.0",
  "description": "AWE CLI - TypeScript command line interface for Awesome Workspace Engineering",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "awe": "./dist/bin/awe.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/bin/awe.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist",
    "setup": "node dist/scripts/setup.js",
    "validate": "node dist/scripts/validate-setup.js"
  },
  "dependencies": {
    "@awe/shared": "workspace:*",
    "@awe/database": "workspace:*",
    "@awe/ai": "workspace:*",
    "@dotenvx/dotenvx": "^1.14.2",
    "@supabase/supabase-js": "^2.48.0",
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^12.1.0",
    "ora": "^8.1.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.6",
    "eslint": "^9.17.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}