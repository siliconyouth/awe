{
  "name": "@awe/cli",
  "version": "2.5.0",
  "description": "AWE CLI - AI-powered development companion for Claude Code optimization",
  "keywords": ["cli", "ai", "claude", "development", "automation", "typescript", "productivity"],
  "homepage": "https://github.com/awe-team/awe#readme",
  "bugs": {
    "url": "https://github.com/awe-team/awe/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awe-team/awe.git",
    "directory": "apps/cli"
  },
  "license": "MIT",
  "author": "AWE Team",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "awe": "./dist/bin/awe.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "build:prod": "tsup --config tsup.config.prod.ts",
    "dev": "tsup --watch",
    "start": "node dist/bin/awe.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist",
    "setup": "node dist/scripts/setup.js",
    "validate": "node dist/scripts/validate-setup.js",
    "prepublishOnly": "pnpm build:prod"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@awe/ai": "workspace:*",
    "@awe/database": "workspace:*",
    "@awe/shared": "workspace:*",
    "@prisma/client": "^6.1.0",
    "@supabase/supabase-js": "^2.48.0",
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "inquirer": "^12.9.2",
    "ora": "^8.2.0",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.10.6",
    "eslint": "^9.17.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}